pipeline{
  agent any
  
  stages {
      stage('Checkout'){
          steps {
               git branch: 'main', url: 'https://github.com/mahendrasimhars-0402/Jenkin-CICD'
          }
        }

        stage('Build'){
          steps {
              echo "Building..."
          }
        }

        stage('Test'){
            steps {
                echo "Testing..."
            }
        }
    }
} 

